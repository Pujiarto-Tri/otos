{
  "version": 2,
  "builds": [
    {
      "src": "otos/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "otos/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "otos.settings",
    "PYTHONPATH": "$PYTHONPATH:.",
    "DEBUG": "False"
  },
  "functions": {
    "otos/wsgi.py": {
      "includeFiles": "**"
    }
  }
}
